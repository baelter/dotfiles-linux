Host *.cloudamqp.com *.84codes.com
  User ubuntu
  ControlPath ~/.ssh/%C
  StrictHostKeyChecking yes
  ProxyCommand /usr/bin/sparoid connect -h %h -P %p
# If you are using the Ruby client, sparoid arguments are passed like this:
# ProxyCommand sparoid connect %h %p
  ProxyUseFdpass yes

Host jump-north
    StrictHostKeyChecking yes
    User ubuntu
    HostName aws-eu-north-1-jumphost.dev.84codes.com
Host jump-us1
    StrictHostKeyChecking yes
    User ubuntu
    HostName aws-us-east-1-jumphost.dev.84codes.com
Host jump-us2
    StrictHostKeyChecking yes
    User ubuntu
    HostName aws-us-east-2-jumphost.dev.84codes.com

Host github.com heroku.com
  ControlMaster auto
  ControlPath ~/.ssh/%r@%h:%p
  ControlPersist 60m
  ServerAliveInterval 20
  LogLevel INFO

Host *
  LogLevel DEBUG
  TCPKeepAlive yes
  Compression no
  ServerAliveInterval 60
  AddKeysToAgent yes
  ControlMaster auto
  ControlPath ~/.ssh/%r@%h:%p
  ControlPersist 2m
  Ciphers aes128-gcm@openssh.com,aes256-gcm@openssh.com,aes256-ctr,aes192-ctr,aes128-ctr
